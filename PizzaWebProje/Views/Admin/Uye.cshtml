@model IEnumerable<PizzaWebProje.Models.Uye>

@{
    ViewBag.Title = "Uye";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Uyeler</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AdSoyad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KullaniciAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sifre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Yetki.Yetki1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AdSoyad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KullaniciAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sifre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Yetki.Yetki1)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delete", new { id=item.UyeId })
        </td>
    </tr>
}

</table>
